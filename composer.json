{
    "name": "itemvirtual/ecommerce-paypal",
    "description": "PayPal's payments with Laravel",
    "keywords": [
        "itemvirtual",
        "ecommerce-paypal"
    ],
    "homepage": "https://github.com/itemvirtual/ecommerce-paypal",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Itemvirtual",
            "email": "projectes@itemvirtual.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "paypal/paypal-checkout-sdk": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Itemvirtual\\EcommercePaypal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itemvirtual\\EcommercePaypal\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itemvirtual\\EcommercePaypal\\EcommercePaypalServiceProvider"
            ],
            "aliases": {
                "EcommercePaypal": "Itemvirtual\\EcommercePaypal\\EcommercePaypalFacade"
            }
        }
    }
}
